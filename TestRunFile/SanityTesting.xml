<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="All Scenarios">
<groups>
<run>
<include name="SanityTest"/>
</run>
</groups>
<listeners>
<listener class-name="LambdaTest.BaseMethods.Listners"/>
</listeners>

  <test thread-count="2" name="Registration Test">
  	<classes>
  	  <class name="LambdaTest.LambdaTestPlaygroud.RegisterScenarios"/>
  	</classes>
  </test>
  
 <test thread-count="2" name="Login Test">
  	<classes>
  	  <class name="LambdaTest.LambdaTestPlaygroud.LoginScenarios"/>
  	</classes>
  </test>

</suite> <!-- Suite -->
